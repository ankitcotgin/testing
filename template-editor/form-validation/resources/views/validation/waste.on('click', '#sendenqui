jQuery(document).on('click', '#sendenquiry', function() {
            jQuery("#msg-box").css('display', 'none');
            var uname = jQuery('#uname').val();
            var uemail = jQuery('#uemail').val();
            var ucompany = jQuery('#ucompany').val();
            var umobile = jQuery('#umobile').val();
            var umessage = jQuery('#umessage').val();
            if (uname == "") {
                $("#msg-box").html('<div class="error-msg">Enter your name</div>');
                $("#msg-box").css('display', 'block');
                $('#uname').focus();
                return false;
            } else if (!uname.match(/^[a-zA-Z ]*$/)) {
                $("#msg-box").html('<div class="error-msg">Name should be alphabetic</div>');
                $("#msg-box").css('display', 'block');
                $('#uname').focus();
                return false;
            } else if (uemail == "") {
                $("#msg-box").html('<div class="error-msg">Please enter your email</div>');
                $("#msg-box").css('display', 'block');
                $('#uemail').focus();
                return false;
            } else if (!uemail.match(/^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/)) {
                $("#msg-box").html('<div class="error-msg">Please enter a valid email address</div>');
                $("#msg-box").css('display', 'block');
                $('#uemail').focus();
                return false;
            } else if (umobile == "") {
                $("#msg-box").html('<div class="error-msg">Please provide a phone number</div>');
                $("#msg-box").css('display', 'block');
                $('#umobile').focus();
                return false;
            } else if ($('#umobile').val().length != 10) {
                $("#msg-box").html('<div class="error-msg">Mobile number must be 10 digits.</div>');
                $("#msg-box").css('display', 'block');
                $('#umobile').focus();
                return false;
            } else if (!umobile.match(/^[6-9]\d{2}\d{3}\d{4}$/)) {
                $("#msg-box").html('<div class="error-msg">Mobile number is not valid!</div>');
                $("#msg-box").css('display', 'block');
                $('#umobile').focus();
                return false;
            } else if (ucompany == "") {
                $("#msg-box").html('<div class="error-msg">Please enter your company name</div>');
                $("#msg-box").css('display', 'block');
                $('#ucompany').focus();
                return false;
            } else if (umessage == "") {
                $("#msg-box").html('<div class="error-msg">Enter your message</div>');
                $("#msg-box").css('display', 'block');
                $('#umessage').focus();
                return false;
            } else {
                $('.show_enquiry').hide();
                $('.loding').show();
                $.ajax({
                    url: "sendmail",
                    method: "POST",
                    data: {
                        mailsubject: 'Mail for contact from arecacorp website',
                        mailname: uname,
                        mailemail: uemail,
                        mailmobile: umobile,
                        mailcompany: ucompany,
                        mailmessage: umessage,
                    },
                    success: function(data) {
                        console.log(data);
                        var data = JSON.parse(data);
                        var msg = data.msg;
                        if (data.status == 1) {
                            // $("#msg-box").html('<div class="success-msg"><i class="fa fa-check text-primary"></i> Thank you!. Your message is successfully sent...</div>');
                            // $("#msg-box").css('display', 'block');
                            // $('.show_enquiry').show();
                            // $('.loding').hide();
                            $('#quotes-form')[0].reset();
                             window.location.replace("https://arecacorp.com/thankyou");
                        } else if (data.status == 0) {
                            $("#msg-box").html('<div class="error-msg">We are sorry, but something went wrong</div>');
                            $("#msg-box").css('display', 'block');
                            $('.show_enquiry').show();
                            $('.loding').hide();
                        }
                    },
                    error: function(xhr, status, error) {
                        console.error(xhr);
                    }
                })
            }
        });